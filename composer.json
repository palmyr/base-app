{
    "name": "palmyr/base-app",
    "description": "Base symfony app to generate cli commands with only some of the core components.",
    "type": "project",
    "license": "GPL-3.0-only",
    "authors": [
        { "name": "Ryan Palm", "email":  "rspalm@bigpond.com" }
    ],
    "autoload": {
        "psr-4": {
            "Palmyr\\App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Palmyr\\App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "palmyr/console": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "bin": [
        "bin/app"
    ]
}
